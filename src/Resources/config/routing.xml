<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="norsys_logs_index" path="/logs">
        <default key="_controller">NorsysLogsBundle:Default:index</default>
    </route>

    <route id="norsys_logs_show" path="/logs/{id}/show">
        <default key="_controller">NorsysLogsBundle:Default:show</default>
    </route>
</routes>
